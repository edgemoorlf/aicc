# Environment Variables for Qwen Implementation
# Copy this file to .env and fill in your actual API credentials

# Alibaba Cloud Access Keys
ALIBABA_ACCESS_KEY_ID=your_access_key_id_here
ALIBABA_ACCESS_KEY_SECRET=your_access_key_secret_here

# Qwen API Configuration
QWEN_API_KEY=your_qwen_api_key_here
QWEN_MODEL=qwen-turbo

# Speech Services Region
ALIBABA_REGION=cn-beijing

# ASR Configuration
ASR_APP_KEY=your_asr_app_key_here

# TTS Configuration  
TTS_APP_KEY=your_tts_app_key_here
TTS_VOICE=Cherry

# Server Configuration
PORT=3003
NODE_ENV=development